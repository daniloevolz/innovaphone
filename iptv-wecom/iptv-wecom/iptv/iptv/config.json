{
  "database": {
    "init": [
      {
        "cmd": "table",
        "name": "channels",
        "init": [
          {
            "cmd": "column",
            "name": "id",
            "type": "BIGSERIAL PRIMARY KEY NOT NULL"
          },
          {
            "cmd": "column",
            "name": "name",
            "type": "VARCHAR(128) UNIQUE NOT NULL"
          },
          {
            "cmd": "column",
            "name": "url",
            "type": "VARCHAR(128) UNIQUE NOT NULL"
          },
          {
            "cmd": "column",
            "name": "img",
            "type": "VARCHAR(128) UNIQUE NOT NULL"
          }
        ]
      }
    ]
  },
  "javascript": {
    "eval": [
      "wecom-iptvservice.js"
    ]
  },
  "apis": {
    "wecom-iptv": {
      "com.innovaphone.launcher": {
        "info": {}
      }
    }
  }
}
